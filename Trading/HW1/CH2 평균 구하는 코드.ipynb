   "source": [
    "f = open(\"sample.txt\")\n",
    "\n",
    "lines = f.readlines()\n",
    "lines\n",
    "\n",
    "f.close()\n",
    "\n",
    "total = 0\n",
    "\n",
    "for line in lines:\n",
    "    score = int(line.replace('\\n',''))\n",
    "    total += score\n",
    "    \n",
    "average = total / len(lines)\n",
    "\n",
    "f = open(\"result.txt\", 'w')\n",
    "f.write(str(average))\n",
    "f.close()"
   ]
